algoritmo "95"
var
   x, y, res: inteiro
funcao somador(a,b: inteiro): inteiro
inicio
   retorne  (a + b)
fimFuncao
inicio
      escreva("Digite o primeiro valor: ")
      leia(x)
      escreva("Digite o segundo valor: ")
      leia(y)
      res <- somador(x,y)
      escreval("A soma é", res)
fimalgoritmo